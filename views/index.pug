doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 Homepage
        .cards 
            each pokemon in data
                .card 
                    h2.card__name #{pokemon.properties.name}
                    p.card__email #{pokemon.properties.weight}
                    p.card__email #{pokemon.properties.height}